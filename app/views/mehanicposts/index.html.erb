<p id="notice"><%= notice %></p>

<h1>Listing Mehanicposts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mehanicposts.each do |mehanicpost| %>
      <tr>
        <td><%= mehanicpost.title %></td>
        <td><%= mehanicpost.description %></td>
        <td><%= mehanicpost.image_url %></td>
        <td><%= link_to 'Show', mehanicpost %></td>
        <td><%= link_to 'Edit', edit_mehanicpost_path(mehanicpost) %></td>
        <td><%= link_to 'Destroy', mehanicpost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mehanicpost', new_mehanicpost_path %>
